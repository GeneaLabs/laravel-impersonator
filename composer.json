{
    "name": "genealabs/laravel-impersonator",
    "description": "Impersonate users in your Laravel 5.4+ app.",
    "license": "MIT",
    "authors": [
        {
            "name": "GeneaLabs, LLC",
            "email": "hello@genealabs.com"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ],
    "require": {
        "php": ">=7.1.3",
        "illuminate/config": "^5.5|^6.0",
        "illuminate/console": "^5.5|^6.0",
        "illuminate/contracts": "^5.5|^6.0",
        "illuminate/http": "^5.5|^6.0",
        "illuminate/routing": "^5.5|^6.0",
        "illuminate/session": "^5.5|^6.0",
        "illuminate/support": "^5.5|^6.0",
        "illuminate/view": "^5.5|^6.0"
    },
    "require-dev": {
        "filp/whoops": "*",
        "fzaninotto/faker": "*",
        "laravel/browser-kit-testing": "*",
        "laravel/laravel": "^6.0",
        "laravel/nova": "*",
        "mockery/mockery": "*",
        "nunomaduro/collision": "*",
        "php-coveralls/php-coveralls" : "*",
        "phpmd/phpmd": "*",
        "phpunit/phpunit": "*",
        "sebastian/phpcpd": "*"
    },
    "autoload": {
        "classmap": [],
        "psr-4": {
            "GeneaLabs\\LaravelImpersonator\\": "src/",
            "GeneaLabs\\LaravelImpersonator\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "GeneaLabs\\LaravelImpersonator\\Providers\\Service",
                "GeneaLabs\\LaravelImpersonator\\Providers\\Tool"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
