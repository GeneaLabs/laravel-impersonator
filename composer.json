{
    "name": "genealabs/laravel-impersonator",
    "description": "Impersonate users in your Laravel 5.4+ app.",
    "license": "MIT",
    "authors": [
        {
            "name": "GeneaLabs, LLC",
            "email": "hello@genealabs.com"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ],
    "require": {
        "illuminate/config": "^8.0",
        "illuminate/console": "^8.0",
        "illuminate/contracts": "^8.0",
        "illuminate/http": "^8.0",
        "illuminate/routing": "^8.0",
        "illuminate/session": "^8.0",
        "illuminate/support": "^8.0",
        "illuminate/view": "^8.0"
    },
    "require-dev": {
        "laravel/browser-kit-testing": "^6.0",
        "laravel/laravel": "^8.0",
        "php-coveralls/php-coveralls" : "^2.2",
        "phpmd/phpmd": "^2.8",
        "phpunit/phpunit": "^9.0",
        "sebastian/phpcpd": "^5.0",
        "laravel/ui": "^3.0",
        "laravel/legacy-factories": "^1.0.4",
        "fzaninotto/faker": "^1.9.1"
    },
    "autoload": {
        "classmap": [],
        "psr-4": {
            "GeneaLabs\\LaravelImpersonator\\": "src/",
            "GeneaLabs\\LaravelImpersonator\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "GeneaLabs\\LaravelImpersonator\\Providers\\Service",
                "GeneaLabs\\LaravelImpersonator\\Providers\\Tool"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
