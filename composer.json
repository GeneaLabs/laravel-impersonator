{
    "name": "genealabs/laravel-impersonator",
    "description": "Impersonate users in your Laravel 5.4+ app.",
    "license": "MIT",
    "authors": [
        {
            "name": "GeneaLabs, LLC",
            "email": "hello@genealabs.com"
        }
    ],
    "require": {
        "php": ">=7.1.3",
        "illuminate/config": "5.5 - 5.7",
        "illuminate/console": "5.5 - 5.7",
        "illuminate/contracts": "5.5 - 5.7",
        "illuminate/http": "5.5 - 5.7",
        "illuminate/routing": "5.5 - 5.7",
        "illuminate/session": "5.5 - 5.7",
        "illuminate/support": "5.5 - 5.7",
        "illuminate/view": "5.5 - 5.7"
    },
    "require-dev": {
        "beyondcode/laravel-dump-server": "^1.0",
        "codedungeon/phpunit-result-printer": "*",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "laravel/browser-kit-testing": "*",
        "laravel/laravel": "5.7.*",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "php-coveralls/php-coveralls" : "*",
        "phpmd/phpmd": "^2.6",
        "phpunit/phpunit": "7.*",
        "sebastian/phpcpd": "*"
    },
    "autoload": {
        "classmap": [],
        "psr-4": {
            "GeneaLabs\\LaravelImpersonator\\": "src/",
            "GeneaLabs\\LaravelImpersonator\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "GeneaLabs\\LaravelImpersonator\\Providers\\Service"
            ]
        }
    },
    "minimum-stability": "stable"
}
